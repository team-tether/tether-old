;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "FSM",
"language": "GDScript",
"path": "res://addons/node_fsm_plugin/fsm_node.gd"
}, {
"base": "Node2D",
"class": "Level",
"language": "GDScript",
"path": "res://Level.gd"
