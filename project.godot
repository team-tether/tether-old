}

[application]

config/name="Tether"
run/main_scene="res://levels/1-1.tscn"
boot_splash/image="res://entities/player/player.png"
boot_splash/fullsize=false
boot_splash/bg_color=Color( 0.141176, 0.141176, 0.141176, 1 )

[autoload]

Game="*res://Game.gd"
SceneChanger="*res://SceneChanger.tscn"
Console="*res://addons/quentincaffeino-console/src/Console.tscn"

[display]

window/size/width=1920
window/size/height=1080
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "fabianlc_scene_palette", "node_fsm_plugin", "quentincaffeino-console" )

